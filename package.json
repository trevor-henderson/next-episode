{
  "name": "next-episode",
  "version": "1.0.0",
  "main": "index.js",
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "."
    ],
    "coverageReporters": [
      "json",
      "html"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": -10
      }
    }
  },
  "scripts": {
    "build": "webpack --mode production",
    "client": "webpack-dev-server --open",
    "server": "nodemon server/index.js",
    "start": "yarn build && node server/index.js",
    "dev_next": "next",
    "build_next": "next build",
    "start_next": "next start",
    "lint": "eslint .",
    "test": "yarn coverage && yarn lint",
    "coverage": "jest --coverage"
  },
  "repository": "https://github.com/trevor-henderson/next-episode.git",
  "author": "Trevor Henderson <trevorsgit@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@hapi/hapi": "^19.1.0",
    "@hapi/inert": "^6.0.1",
    "next": "^9.2.1",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "styled-components": "^5.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/polyfill": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "@testing-library/jest-dom": "^5.1.1",
    "@testing-library/react": "^9.4.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.0.6",
    "chance": "^1.1.4",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.4.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^2.3.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^25.1.0",
    "nodemon": "^2.0.2",
    "style-loader": "^1.1.3",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.3"
  }
}
